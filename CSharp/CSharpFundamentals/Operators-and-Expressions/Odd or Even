using System;


namespace OddOrEven
{
    class OddOrEven
    {
        static void Main()
        {
            int number = int.Parse(Console.ReadLine());
            int isOdd;

            if (number % 2 == 0)
            {
                Console.WriteLine("even {0}", number);
            }
            else {
                Console.WriteLine("odd {0}", number);
            }

        }
    }
}
